  float tn2;
  float tk2;
  float tn3;
  float tk3;
  float tn4;
  float tk4;
  float tab;

};


//МОЯ СТРУКТУРА КА
struct Kosmo
{
	public:
	int name;
	double r[6];//вектор  время в секундах
